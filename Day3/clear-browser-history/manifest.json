{
  "name": "Clear Your History",
  "description": "Clear your broswer history with one simple click!",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Click to clear your history",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./32x32.png",
    "32": "./32x32.png",
    "48": "./32x32.png",
    "128": "./32x32.png"
  },
  "permissions": ["history"]
}
